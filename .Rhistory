USA93 <- Cars93 %>% filter(Origin == 'USA') %>% dplyr::select(Price, MPG)
USA93 <- Cars93 %>% dplyr::filter(Origin == 'USA') %>% dplyr::select(Price, MPG)
non_USA93 <- Cars93 %>% dplyr::filter(Origin == 'non-USA') %>% dplyr::select(Price)
#2
USA93 <- Cars93 %>% dplyr::filter(Origin == 'USA') %>% dplyr::select(Price)
non_USA93 <- Cars93 %>% dplyr::filter(Origin == 'non-USA') %>% dplyr::select(Price)
shapiro.test(mtcarsA0$mpg)
shapiro.test(USA93)
shapiro.test(non_USA93)
shapiro.test(USA93$Price)
shapiro.test(non_USA93$Price)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(formula = non_USA93$Price ~ USA93$Price)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(formula = Cars93$Price ~ Cars93$Origin)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(Cars93$Price ~ Cars93$Origin)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(Cars93$Price ~ Cars93$Origin)
# 정규성을 가진다고 가정하고 two sample - test를 진행한다.
t.test(non_USA93$Price, USA93$Price, var.equal = F)
# 등분산성 테스트
var.test(non_USA93$Price, USA93$Price)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93$Price ~ USA93$Price)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93 ~ USA93)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(Cars93$Price ~ Cars93$Origin)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93 ~ USA93, conf.int = F)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93$Price ~ USA93$Price, conf.int = F)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93$Price ~ USA93$Price, conf.int = FALSE)
# 정규성이 아닐 시 Wilcoxon rank sum - test를 실시한다.
wilcox.test(non_USA93$Price ~ USA93$Price, alternative = c("two.sided"), mu = 0, conf.int = FALSE, conf.level = 0.95)
Price_USA <- Cars93[which(Cars93$Origin == c("USA")), c("Price")]
